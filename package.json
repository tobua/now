{
  "name": "create-now",
  "description": "Create projects from templates.",
  "version": "0.0.0-development",
  "repository": "github:tobua/now",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "test": "padua test"
  },
  "padua": {
    "source": true
  },
  "dependencies": {
    "download-git-repo": "^3.0.2",
    "ejs": "^3.1.6",
    "find-cache-dir": "^3.3.2",
    "isbinaryfile": "^4.0.8",
    "logua": "^2.1.4",
    "node-fetch": "^3.2.2",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "temp-dir": "^2.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "type": "module",
  "sideEffects": true,
  "main": "index.js",
  "exports": {
    "default": "./index.js"
  },
  "bin": "cli.js",
  "source": "index.js",
  "types": "index.d.ts",
  "files": [
    "utility",
    "*.js",
    "*.d.ts",
    "!test"
  ],
  "keywords": [
    "template",
    "download",
    "generator",
    "init"
  ],
  "devDependencies": {
    "is-png": "^3.0.1",
    "jest-fixture": "^3.0.0",
    "mock-stdin": "^1.0.0",
    "padua": "^0.5.3",
    "read-chunk": "^4.0.2"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "import/extensions": [
        2,
        "ignorePackages"
      ]
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "test/fixture"
    ],
    "transform": {
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/cint"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}
